{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2015",
    "module": "NodeNext",
    "strict": true,
    "moduleResolution": "NodeNext",
    "lib": ["es2016", "dom", "es5", "dom.iterable"],
    "sourceMap": true,
    "declarationMap": true,
    "paths": {
      "nodemodules/*": [
        "../node_modules/*"
      ],
      "frontend/*": ["../bazel-bin/frontend/*"],
    },
    "declaration": true,
    "allowJs": true,
  }
}